import sqlite3

conn = sqlite3.connect('data.db')

conn.execute('''CREATE TABLE InovClientsData
            (name VARCHAR(255),
            email VARCHAR(255),
            SDC VARCHAR(255),
            DCL  VARCHAR(250),
            Type VARCHAR(255),
            Current INT(150),
            Saving INT(150),
            Address VARCHAR(255),
            Country VARCHAR(255),
            Currency VARCHAR(255))''')

print("table created respectfully")

conn.close()
